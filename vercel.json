{
  "functions": {
    "api/booking/parse.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/booking/url-import.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/template-selector.html", "dest": "/template-selector.html" },
    { "src": "/template-configurator.html", "dest": "/template-configurator.html" },
    { "src": "/template-generator.html", "dest": "/template-generator.html" },
    { "src": "/test-templates.html", "dest": "/test-templates.html" },
    { "src": "/templates/(.*)", "dest": "/templates/$1" }
  ],
  "rewrites": [
    { 
      "source": "/:brandSlug((?!api|templates|js|styles|images|public)[^/]+)/:pageSlug", 
      "destination": "/api/pages/:brandSlug/:pageSlug"
    }
  ],
  "headers": [
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/styles/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/preview.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "installCommand": "echo Skip install",
  "buildCommand": "echo Skip build",
  "outputDirectory": "."
}
