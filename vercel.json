{
  "functions": {
    "api/booking/parse.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/booking/url-import.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/template-checklist",
      "destination": "/template-checklist.html"
    },
    {
      "source": "/template-checklist.html",
      "destination": "/template-checklist.html"
    },
    { 
      "source": "/:brandSlug((?!api|template-checklist)[^/.]+)/:pageSlug",
      "destination": "/api/pages/:brandSlug/:pageSlug"
    }
  ],
  "headers": [
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/styles/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/preview.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "installCommand": "echo Skip install",
  "buildCommand": "echo Skip build",
  "outputDirectory": ".",
  "public": true
}
